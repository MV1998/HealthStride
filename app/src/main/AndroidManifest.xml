<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mohit.varma.healthstride">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".healthtopics.Injuries"
            android:parentActivityName=".MainActivity"
            android:label="Injuries"></activity>
        <activity android:name=".healthtopics.FoodSafety"
            android:parentActivityName=".MainActivity"
            android:label="Food Safety"></activity>
        <activity android:name=".healthtopics.Depression"
            android:parentActivityName=".MainActivity"
            android:label="Depression"></activity>
        <activity android:name=".healthtopics.SleepDifficulties"
            android:parentActivityName=".MainActivity"
            android:label="Sleep Difficulties"></activity>
        <activity android:name=".healthtopics.Stress"
            android:parentActivityName=".MainActivity"
            android:label="Stress"></activity>
        <activity android:name=".healthtopics.BloodPressure"
            android:parentActivityName=".MainActivity"
            android:label="Blood Pressure"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>